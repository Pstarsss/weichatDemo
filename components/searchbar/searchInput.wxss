/* components/searchInput.wxss */
.searchbar {
  width: 100%;
  height: 90rpx;
  padding: 12rpx;
  background-color: var(--themeColor);
  display: flex;
  justify-content: center;
  align-items: center;
}
.searchbar navigator {
  color: #666;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  width: 96%;
  height: 90%;
  border-radius: 20rpx;
}
